#!/usr/bin/env python
from llm_grabbing_bringup.action_lib import move_to_position, close_gripper, open_gripper

def main():
    move_to_position('default')   
    move_to_position('yellow_cube_4')
    close_gripper()
    move_to_position('yellow_cube_4_top')
    # move_to_position('yellow_cube_4_end')
    open_gripper()
    move_to_position('default')
if __name__ == '__main__':
    main()